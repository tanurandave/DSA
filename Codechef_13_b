import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef_13_b
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		
		List<Integer> list=new ArrayList<>();
		
		for(int i=0; i<n; i++){
		    list.add(sc.nextInt());
		}
		
		for(int i=n-1; i>=0;i--){
		    System.out.print(list.get(i)+" ");
		}

	}
}
