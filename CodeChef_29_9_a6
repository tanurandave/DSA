import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here

     Scanner sc=new Scanner(System.in);
     int X=sc.nextInt();
     
     if(X+7>170){
         System.out.println("Yes");
     }
     else{
         System.out.println("No");
     }
	}
}
