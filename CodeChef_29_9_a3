import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();

        while(T-->0){
            int N=sc.nextInt();
            int M=sc.nextInt();
            int K=sc.nextInt();
            
            if(N+K<=M)
                System.out.println("Yes");
            else
                System.out.println("No");
        }
	}
}
