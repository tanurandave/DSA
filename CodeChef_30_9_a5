import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here

        Scanner sc=new Scanner(System.in);
        int T=sc.nextInt();
        
        while(T-->0){
            int X=sc.nextInt();
            
            int percentDiscount=X/10;
            int flatDiscount=100;
            
            int result= Math.max(percentDiscount, flatDiscount);
            System.out.println(result);
        }
	}
}
